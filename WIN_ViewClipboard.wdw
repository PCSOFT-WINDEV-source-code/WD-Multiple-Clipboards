     name : OnShortcut
     internal_properties : CAAAAAgAAABFTqLhB+0MSUE3mwptLx8kruDDcyY7+GWDbuSilWbveYGTV4wCWHKle99e7czBuarkzGmZm77y3NIMvt8+VdT3NcueeZOkpCbBvrFdBNf919lprFJw0pNwr39ELJOrqmTlZAONn+2IpTlJ9TRHTBUEbklx1BWrI1J38ZUj3sUTohc9nXCcVUwYnqm1DbmuYJovbYC15ZCZqViHLU7fC7o+rhNV3ZEcrVAwA6OxERCQNIEw6iclWdc0p+4+rIFUwGFS0g==
     procedure_id : 1355123097456639643
     type_code : 14
     code : |1+
      // Summary: Process of the WM_HOTKEY message
      // Parameters:
      //	nMessage (system int): WM_HOTKEY
      //	nWParam (system int): hotkey identifier (specified during the call to RegisterHotKey)
      //	nLParam (system int  modification key (low word), virtual key (high word)
      //
      Procedure OnShortcut( nMessage is system int , nWParam is system int , nLParam is system int <useful> )
      
      dbgAssert(nMessage=WM_HOTKEY)
      
      IF nWParam=ID_SHORTCUT_MULTIPLE_PASTE THEN
      	// Find the window with the caret
      	stInfo is GUITHREADINFO
      	stInfo.nCbSize = Dimension(stInfo)
      	IF GetGUIThreadInfo(Null,&stInfo) THEN
      		// Check it is a WinDev application
      		// Note: other applications correctly support sending a WM_PASTE message but not all
      		// for the scope if this example, we restrain ourselves to WinDev application
      		IF GetClassName(stInfo.nHwndActive)=CLASS_WINDOW_WINDEV THEN
      			// Calculate the position of the popup to open
      			nHwndTarget is system int = stInfo.nHwndCaret
      			stCorner is STPOINT
      			IF stInfo.nHwndCaret=Null THEN
      				nHwndTarget = stInfo.nHwndFocus
      				GetWindowRect(stInfo.nHwndFocus,&stInfo.rcCaret)
      			END
      			stCorner.x = stInfo.rcCaret.nLeft
      			stCorner.y = stInfo.rcCaret.nTop
      			// If the position was obtained by hwndCaret, it is expressed in client coordinates of the window
      			// it needs to be converted in screen coordinates
      			IF stInfo.nHwndCaret<>Null THEN
      				ClientToScreen(stInfo.nHwndCaret,&stCorner)		
      			END
      			// Opens the popup for selecting a section at caret location
      			nElementHistory is int = Open(WIN_PopupFragmentSelection,stCorner)
      			IF nElementHistory<>-1 THEN
      				// Give the focus back to the right window
      				SysSetFocus(stInfo.nHwndActive)
      				GetElement(nElementHistory)
      				// Send a paste message
      				SendMessage(nHwndTarget,WM_PASTE,0,0)
      			END
      		END
      	END
      END
     type : 458752
   -
     name : FillClipboard
     procedure_id : 1355123097456705179
     type_code : 14
     code : |1-
      // Summary: Retrieves an element in the history and places it in the system clipboard
      // Parameters:
      //	nElementHistory (int): rank of the element to retrieve
      // Return value:
      // 	None
      //
      Procedure FillClipboard(nElementHistory is int )
      
      dbgAssert(nElementHistory<=ArrayCount(garrClipboardElem))
      
      gbClipboardModificationInProgress = True
      
      // Open the clipboard
      IF OpenClipboard(Handle())=False THEN 
      	RETURN
      END
      
      EmptyClipboard()
      
      // For each saved format
      FOR EACH stFormat of garrClipboardElem[nElementHistory].arrElemFormat
      	// Copy the information in a global memory area
      	nHMem is system int = GlobalAlloc(GMEM_MOVEABLE,stFormat.nSize)
      	IF nHMem<>Null THEN
      		nPtrMem is system int = GlobalLock(nHMem,stFormat.nSize)
      		IF nPtrMem<>Null THEN
      			Transfer(nPtrMem,&stFormat.bufContent,stFormat.nSize)
      			GlobalUnlock(nHMem)
      			// Add to the clipboard
      			SetClipboardData(stFormat.nFormat,nHMem)	
      		END
      		// Frees the memory block
      		GlobalFree(nHMem)
      	END	
      END
      
      // Close the clipboard
      CloseClipboard()
      
      END:
      gbClipboardModificationInProgress = False
     type : 458752
   -
     name : InitTable
     procedure_id : 1355123097456770715
     type_code : 14
     code : |1-
      // Summary: Fill the table using the elements in memory
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      Procedure InitTable()
      
      TableDeleteAll(TABLE_ViewClipboard)
      
      FOR EACH stElement of garrClipboardElem
      	AddRowIntoTable(stElement)
      END
     type : 458752
   -
     name : InitializeElement
     procedure_id : 1355123097456836251
     type_code : 14
     code : |1-
      // Summary: initialize the table of elements by adding the clipboard's content except if it's already there, for example in the case where the application is restarted
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      Procedure InitializeElement()
      
      // Initialize the table
      InitTable()
      
      // During the initialization, we must copy the clipboard content (if it exists)
      CopyClipboardContent()
      
      // if the two last elements are identical, delete the last one
      IF ArrayCount(garrClipboardElem)>1 THEN
      	nLast is int = ArrayCount(garrClipboardElem)
      	nBeforeLast is int = nLast-1
      	IF garrClipboardElem[nLast].bufPreview = garrClipboardElem[nBeforeLast].bufPreview THEN
      		IF ArrayCount(garrClipboardElem[nLast].arrElemFormat) = ArrayCount(garrClipboardElem[nBeforeLast].arrElemFormat) THEN
      			FOR n=1 _TO_ ArrayCount(garrClipboardElem[nLast].arrElemFormat)
      				IF garrClipboardElem[nLast].arrElemFormat[n] <> garrClipboardElem[nLast].arrElemFormat[n] THEN
      					RETURN
      				END
      			END
      			// Delete the array's last element
      			ArrayDeleteLine(garrClipboardElem,ArrayCount(garrClipboardElem))
      			// and the table's first one
      			TableDelete(TABLE_ViewClipboard,1)
      		END	
      	END
      END
     type : 458752
   -
     name : AddRowIntoTable
     procedure_id : 1355123097456901787
     type_code : 14
     code : |1-
      // Summary: Adds a history element into the visualization table
      // Parameters:
      //	stElement: New history element
      //
      Procedure AddRowIntoTable(stElement)
      
      TableInsertLine(TABLE_ViewClipboard,1,stElement.bufPreview)
      SWITCH stElement.nPreviewType
      	CASE PREVIEW_TEXT
      		sText is string = stElement.bufPreview
      		TABLE_ViewClipboard[1].STC_Preview = sText
      		TABLE_ViewClipboard[1].STC_Preview..Visible = True
      		TABLE_ViewClipboard[1].IMG_Preview..Visible = False
      	CASE PREVIEW_IMAGE
      		TABLE_ViewClipboard[1].IMG_Preview = stElement.bufPreview
      		TABLE_ViewClipboard[1].STC_Preview..Visible = False
      		TABLE_ViewClipboard[1].IMG_Preview..Visible = True
      	OTHER CASE
      END
