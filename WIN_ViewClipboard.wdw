      // Resume: Process the actions on the systray icon
      // Parameters:
      //	nOperation (int): type of operation performed by the user
      // Return value:
      // 	None
      //
      Procedure OnSystray( nOperation is int )
      
      IF nOperation=WM_LBUTTONDBLCLK THEN
      	MyWindow..Visible = True
      	MENU_PopupMenu.OPT_MinimizeRestore..Caption = <§$0001§>
      	ReturnToCapture()
      END
     type : 458752
   -
     name : GetElement
     procedure_id : 1355123097457032859
     type_code : 14
     code : |1-
      // Summary: Moves an element at the bottom of the history table and copies it into the system clipboard
      // Parameters:
      // 	nElementHistory: rank of the element to retrieve 
      //
      Procedure GetElement(nElementHistory)
      
      IF garrClipboardElem..Occurrence=0 THEN RETURN
      
      // Place the selected element at the bottom of the history if needed
      IF nElementHistory<>ArrayCount(garrClipboardElem) THEN
      	ArrayMoveLine(garrClipboardElem,nElementHistory,ArrayCount(garrClipboardElem),amMove)
      	TableMoveLine(TABLE_ViewClipboard,TableCount(TABLE_ViewClipboard)-nElementHistory+1,1,amMove)
      	// Retrieve the history element
      	FillClipboard(ArrayCount(garrClipboardElem))
      END
