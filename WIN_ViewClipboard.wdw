     code : |1+
      
      // Add the window to the clipboard observers string
      gnHandleNextObserver is system int
      gnHandleNextObserver = SetClipboardViewer(Handle())
      gbClipboardModificationInProgress is boolean = False
      
      LOCAL
      	
      	// Branch the events 
      // WM_DRAWCLIPBOARD is received every time the clipboard is modified
      Event("OnClipboardChange","*.",WM_DRAWCLIPBOARD)
      // WM_CHANGECBSTRING is received every time the string of clipboard observers is modified
      Event("OnChangeClipboardString","*.",WM_CHANGECBCHAIN)
      // WM_HOTKEY is received each time a keyboard shortcut is pressed
      Event("OnShortcut","*.",WM_HOTKEY)
      
      // Add the global shortcut CTRL+SHIFT+V
      RegisterHotKey(Handle(),ID_SHORTCUT_MULTIPLE_PASTE,MOD_CONTROL+MOD_SHIFT,VK_V)
      
      // Initialize the table of elements by adding the clipboard's content
      InitializeElement()
      
      // Delete the icon from the systray
      SysIconAdd("clipboard.ico",MENU_PopupMenu,"","OnSystray")
