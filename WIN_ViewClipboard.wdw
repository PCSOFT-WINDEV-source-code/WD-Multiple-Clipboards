   -
     name : OnChangeClipboardString
     procedure_id : 1355123097456443035
     type_code : 14
     code : |1+
      // Summary: Process the WM_CHANGECBSTRING message 
      // Parameters:
      //	nMessage (system int): WM_CHANGECBSTRING
      //	nWParam (system int): handle of the window removed from the clipboard observers string
      //	nLParam (system int): handle of the next window in the string of clipboard observers
      // Return value:
      // 	integer: // 	None
      //
      Procedure OnChangeClipboardString( nMessage is system int , nWParam is system int , nLParam is system int )
      
      dbgAssert(nMessage=WM_CHANGECBCHAIN )
      
      // If it is the next window in the string that is removed, we need to link to its successor
      IF nWParam=gnHandleNextObserver THEN
      	gnHandleNextObserver = nLParam
      END
      
      // Pass the message to the next observer if there's one
      IF gnHandleNextObserver<>Null THEN
      	SendMessage(gnHandleNextObserver, nMessage, nWParam, nLParam)
      END
      
      // The message is processed, we need to return 0
      RESULT 0
     type : 458752
   -
     name : OnClipboardChange
     procedure_id : 1355123097456508571
     type_code : 14
     code : |1+
      // Summary: The content of the clipboard has changed
      // Parameters:
      //	nMessage (system int): WM_DRAWCLIPBOARD
      //	nWParam (system int): not used, must be null
      //	nLParam (system int): not used, must be null
      // Return value:
      // 	None
      //
      Procedure OnClipboardChange( nMessage is system int , nWParam is system int , nLParam is system int )
      
      dbgAssert(nMessage=WM_DRAWCLIPBOARD)
      
      // If it's not this application currently modifying the clipboard
      IF gbClipboardModificationInProgress =False THEN
      	// The clipboard's content must be copied locally
      	CopyClipboardContent()
      END
      
      // Pass the message to the next window in the clipboard observer string
      IF gnHandleNextObserver<>Null THEN
      	SendMessage(gnHandleNextObserver,WM_DRAWCLIPBOARD,nWParam,nLParam)
      END
     type : 458752
   -
     name : CopyClipboardContent
     internal_properties : CAAAAAgAAAA0j3DGSTD6ccbmS2Wq6svu88CUvMJ/ea70/mAVVaAiv5IHgZypFrQ1zOSG6HaS1dr4wMRFJrJh4wQgwS7MiLxx45SqkqQeo4Iags7uWMGC6XoHfUnxem/8mDVwqm9oKlNftsVH+N2hSlzfuoFojN2EIQcD1izBpLu0Obhi4VXrWjLsrhZTYsGcQJUHBbj4wSMlfplUz8Q=
     procedure_id : 1355123097456574107
     type_code : 14
     code : |1+
      // Summary: retrieve the clipboard content and add it to the history
      //
      Procedure CopyClipboardContent()
      
      // If the clipboard is empty, exit right away
      IF CountClipboardFormats()=0 THEN
      	RETURN
      END
      
      // Open the clipboard
      IF OpenClipboard(Handle())=False THEN 
      	RETURN
      END
      
      bAddGUI is boolean = False
      stElement is ST_ELEM_CLIPBOARD
      nNbFormat is int = CountClipboardFormats()
      stElement.nPreviewType = PREVIEW_TEXT
      
      // Read the format saved in the clipboard
      nDataHandle is system int
      nPtr is system int
      nFormatRank is int = 1
      nFormat is int = EnumClipboardFormats(0)
      bPreviewFound is boolean = False
      bSupportedFormat is boolean
      WHILE nFormat<>0
      	// Initialize the structure
      	stFormat is ST_ELEM_CLIPBOARD_FORMAT
      	stFormat.nFormat = nFormat
      	bSupportedFormat = False
      	// Format name
      	stFormat.sFormatName = GetClipboardFormatName(stFormat.nFormat)
      	
      	// Retrieve a global handle on the data
      	nDataHandle = GetClipboardData(nFormat)
      	// Map the process in memory and read the size
      	nPtr = GlobalLock(nDataHandle,stFormat.nSize)
      	
      	// Depending on the format found
      	SWITCH nFormat
      		CASE CF_TEXT
      			bSupportedFormat = True
      		CASE CF_DIB
      			stElement.nPreviewType = PREVIEW_IMAGE
      			bSupportedFormat = True
      		OTHER CASE
      			// This format is not supported, ignore it
      	END
      	
      	// Free the global handle
      	GlobalUnlock(nDataHandle)
      	
      	// If the format is supported by the application, it is added to the array
      	IF bSupportedFormat THEN
      		// Copy the data
      		stFormat.bufContent = RepeatString(Charact(0),stFormat.nSize+1)
      		Transfer(&stFormat.bufContent,nPtr,stFormat.nSize)
      		stElement.bufPreview = stFormat.bufContent
      		// Add the format to the current element
      		ArrayAdd(stElement.arrElemFormat,stFormat)
      	END
      	
      	// Next format
      	nFormatRank++
      	nFormat = EnumClipboardFormats(nFormat)
      END
      
      // Close the clipboard
      CloseClipboard()
      
      // If the preview is an image, we need to retrieve it (the process is executed after the loop because the clipboard handling functions cannot
      // be used between OpenClipboard and CloseClipboard
      IF stElement.nPreviewType = PREVIEW_IMAGE THEN
      	IMG_GetPreview..Width = ClipboardWidth()
      	IMG_GetPreview..Height = ClipboardHeight()
      	Clipboard(IMG_GetPreview)
      	stElement.bufPreview = dSaveImageJPEG(IMG_GetPreview,inMemory)
      END
      
      // Add the element in the element table
      ArrayAdd(garrClipboardElem,stElement)
      
      // If the addition into the table was not performed yet, add a generic text
      IF bAddGUI=False THEN
      	AddRowIntoTable(stElement)
      END
     type : 458752
