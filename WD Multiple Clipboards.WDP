     code : |1-
      CONSTANT
      	ID_SHORTCUT_MULTIPLE_PASTE = 1000
      	PREVIEW_TEXT = 1
      	PREVIEW_IMAGE = 2
      	CLASS_WINDOW_WINDEV = "WinDevObject"
      END
      
      // Structure containing the format of a clipboard element
      ST_ELEM_CLIPBOARD_FORMAT is Structure
      	nFormat is int
      	sFormatName is string
      	nSize is int
      	bufContent is buffer	
      END
      
      // Structure containing a clipboard element (all formats)
      ST_ELEM_CLIPBOARD is Structure
      	arrElemFormat is array of ST_ELEM_CLIPBOARD_FORMAT
      	bufPreview is buffer
      	nPreviewType is int
      END
      
      // Array of clipboard elements
      garrClipboardElem is array of ST_ELEM_CLIPBOARD
      
      // Deserialize the clipboard
      gsBackupDir is string = fDataDirUser()+["\"]+<ยง@00000000000000000000ยง>
      gsBackupFile is string = gsBackupDir+<ยง@00000000000000000001ยง>
      IF fFileExist(gsBackupFile) THEN
      	WHEN EXCEPTION IN
      		LOCAL
      			bufBackup is buffer = RepeatString(Charact(0),fSize(gsBackupFile))
      			nFil is int = fOpen(gsBackupFile,foRead)
      		fRead(nFil,fSize(gsBackupFile),&bufBackup)
      		fClose(nFil)
      		Deserialize(garrClipboardElem,bufBackup,psdBinary)
      	DO
      		fDelete(gsBackupFile)
      	END
      END
     type : 196608
   -
     code : |1-
      
      IF fMakeDir(gsBackupDir) THEN
      	bufBackup is buffer
      	Serialize(garrClipboardElem,bufBackup,psdBinary)
      	nFil is int = fCreate(gsBackupFile)
      	IF nFil>0 THEN
      		nSize is int = Length(bufBackup)
      		fWrite(nFil,&bufBackup,nSize)
      		fClose(nFil)
      	END
      END
